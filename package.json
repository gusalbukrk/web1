  "scripts": {
    "build": "tsc && webpack --node-env production --config webpack.prod.js",
    "cspell": "cspell \"{**,**/.*,**/.*/**}\" -c cspell.config.cjs --no-progress",
    "lint": "npm run cspell && npm run lint:config && prettier **/*.{md,json} --write --loglevel warn && npm run lint:src",
    "lint:config": "eslint . --fix --ext .js,.cjs,.json --ignore-pattern '!.*' --ignore-pattern 'src/'",
    "lint:src": "eslint src/ --fix && stylelint \"src/**/*.{css,scss,sass}\"",
    "prepare": "husky install",
    "start": "webpack serve --node-env development --config webpack.dev.js",
    "test": "jest --passWithNoTests"
  },
